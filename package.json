{
  "name": "rainman",
  "version": "1.5.1",
  "description": "Weather forecast information",
  "main": "lib/index.js",
  "author": "Joe McKie <joe@joemck.ie>",
  "license": "MIT",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src && npm run flow",
    "lint:staged": "lint-staged",
    "test": "npm run lint && npm run test:mocha",
    "test:mocha": "mocha --compilers js:babel-core/register --require babel-polyfill src/__tests__/**/*.spec.js",
    "coverage": "nyc --check-coverage --lines 95 --functions 95 --branches 95 npm test",
    "build": "babel src -d lib --ignore __tests__",
    "prepublish": "BABEL_ENV=production npm run build"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint:staged",
    "coverage"
  ],
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babili": "^0.0.9",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-mocha": "^4.7.0",
    "flow-bin": "^0.37.0",
    "lint-staged": "^3.2.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.11",
    "nyc": "^15.0.0",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
